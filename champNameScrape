import requests
import json
import logging
import inspect
from bs4 import BeautifulSoup

r = requests.get("http://leagueoflegends.wikia.com/wiki/List_of_champions")
# "http://champion.gg/champion/Leblanc"

soup = BeautifulSoup(r.content, "html.parser")
#print soup.prettify()
championTable = soup.find("table",attrs={'class':'wikitable sortable'})
# HTML table of champion names
data = []
rows = championTable.find_all('tr')
for row in rows:
    cols = row.find_all('td')
    for col in cols:
        col = col.find_all('span',attrs={'class','sortkey'})
        if col != []:
            col = col[0]
            col = col.text
            data.append(col)
            print col

newData = [data[i] for i in range(0,len(data),2)]
print newData
